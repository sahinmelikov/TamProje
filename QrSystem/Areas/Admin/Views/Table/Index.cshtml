@model IEnumerable<RestourantTables>
<div class="container">
    <a class="btn btn-primary" asp-action="Create">Create</a>
    <table class="table table-bordered table-responsive">
        <thead>
            <tr>

                <th>Id</th>
                <th>TableNumber</th>
                <th>Ofisant</th>
                <th>QrCodeId</th>
               <th>RestorantName</th>
                <th>CreateTime</th>

                <th colspan="2">Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in (Model ?? new List<RestourantTables>()))
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.TableNumber</td>
                    <td>@item.Ofisant.Name</td>
                    <td>@item.QrCodeId</td>
                    <td>@item.QrCode.Restorant.RestorantName</td>
                    <td>@item.DateTime</td>
                    <td><a class="btn btn-warning" asp-action="Update" asp-route-id="@item.Id">Update</a></td>
                    <td><a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>