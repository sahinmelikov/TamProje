                <script>
                    function submitForm(event) {
                        // Formun submit işlemini engelle
                        event.preventDefault();

                        // Butonu devre dışı bırak
                        var button = document.getElementById('submitButton');
                        button.disabled = true;

                        // FormData objesi oluştur
                        var formData = new FormData(document.getElementById('checkoutForm'));

                        // AJAX ile formu submit et
                        fetch(document.getElementById('checkoutForm').action, {
                            method: 'POST',
                            body: formData
                        })
                            .then(response => {
                                if (response.ok) {
                                    // Başarılı yanıt alındığında modalı aç
                                    var modal = new bootstrap.Modal(document.getElementById('exampleModal'));
                                    modal.show();
                                } else {
                                    // Hata durumunda mesajı göster
                                    alert('Sipariş onaylanamadı.');
                                }
                            })
                            .catch(error => {
                                // Hata durumunda mesajı göster
                                console.error('Hata:', error);
                                alert('Sipariş onaylanamadı.');
                            });
                    }
                </script>
                <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
                <script>
                    $(document).ready(function () {
                        $('.dec, .inc').on('click', function (e) {
                            e.preventDefault(); // Butona tıklama olayını engelle

                            var form = $(this).closest('form');
                            var quantityInput = form.find('input[name="quantity"]');
                            var productId = form.find('input[name="productId"]').val();
                            var qrCodeId = form.find('input[name="qrCodeId"]').val();
                            var action = $(this).val();

                            var currentValue = parseInt(quantityInput.val());

                            if (action === "increment") {
                                quantityInput.val(currentValue + 1);
                            } else if (action === "decrement" && currentValue > 0) {
                                quantityInput.val(currentValue - 1);
                            }

                            $.ajax({
                                url: form.attr('action'),
                                type: 'POST',
                                data: {
                                    productId: productId,
                                    qrCodeId: qrCodeId,
                                    action: action,
                                    quantity: quantityInput.val()
                                },
                                success: function (response) {
                                    console.log('Miktar başarıyla güncellendi.');
                                    // 1 saniye sonra sayfayı yenile
                                    setTimeout(function () {
                                        location.reload();
                                    }, 10);
                                },
                                error: function (xhr, status, error) {
                                    console.error('Miktar güncellenirken bir hata oluştu:', error);
                                }
                            });
                        });
                    });
                </script>

